root = <%= node.rails.app_root %>
upstream <%= node.rails.app_name %> {
  server unix:/tmp/unicorn.sock
  fail_timeout=0;
}
server {
    listen 80 default;
    
    access_log "#{root}/log/nginx_access.log";
    error_log "#{root}/log/nginx_error.log";
    keepalive_timeout 10;
    root "#{root}/current";
}